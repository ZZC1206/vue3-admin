{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "paths": {
      "@/*": [
        "src/*"
      ]
    },
    "baseUrl": "./",
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.js",
    "src/**/*.d.ts",
    "src/**/*.vue",
    "vite.config.ts",
    ".eslintrc.cjs",
    "auto-imports.d.ts",
    "components.d.ts"
  ]
}